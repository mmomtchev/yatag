    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch node-gdal-async",
            "cwd": "${workspaceFolder}/test/node-gdal-async/",
            "args": [ "-v" ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/yatag.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch ndarray-gdal",
            "cwd": "${workspaceFolder}/test/ndarray-gdal/",
            "args": [ "-v" ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/yatag.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Queue",
            "cwd": "${workspaceFolder}/test/Queue/",
            "args": [
                "-v"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/yatag.js"
        }
    ]
}
